<h2>Edit Sub</h2>

<form class="" action="<%= sub_url(@sub) %>" method="post">
  <input type="hidden" name="_method" value="patch">
  <label>
    Title
    <input type="text" name="sub[title]" value="<%= @sub.title %>">
  </label>

  <br>

  <label>
    Description
    <input type="text" name="sub[description]" value="<%= @sub.description %>">
  </label>
  <br>
  <input type="submit" name="" value="Update Sub">
</form>
